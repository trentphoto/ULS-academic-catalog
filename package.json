    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "i": "^0.3.6",
    "node-sass-chokidar": "^1.3.3",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
